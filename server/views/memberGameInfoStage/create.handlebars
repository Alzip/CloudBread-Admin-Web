<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New Member Game Info
        <small>Create a Member Game Info</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'memberGameInfoStage'}}}">List</a></li>
        <li class="active">New Member Game Info</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'memberGameInfoStage.store'}}}" method="post">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Member Game Info</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[MemberGameInfoStageID]" placeholder="MemberGameInfoStageID" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[MemberID]" placeholder="MemberID" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[StageName]" placeholder="StageName" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[StageStatus]" placeholder="StageStatus" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Points]" placeholder="Points" required/>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Category</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Category1]" placeholder="Category1" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Category2]" placeholder="Category2" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Category3]" placeholder="Category3" required/>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Stage Stat</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[StageStat1]" placeholder="StageStat1"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[StageStat2]" placeholder="StageStat2"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[StageStat3]" placeholder="StageStat3"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[StageStat4]" placeholder="StageStat4"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[StageStat5]" placeholder="StageStat5"/>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Mission</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Mission1]" placeholder="Mission1"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Mission2]" placeholder="Mission2"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Mission3]" placeholder="Mission3"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Mission4]" placeholder="Mission4"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="memberGameInfoStage[Mission5]" placeholder="Mission5"/>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Extra Data</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol1]" placeholder="sCol1"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol2]" placeholder="sCol2"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol3]" placeholder="sCol3"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol4]" placeholder="sCol4"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol5]" placeholder="sCol5"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol6]" placeholder="sCol6"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol7]" placeholder="sCol7"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol8]" placeholder="sCol8"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol9]" placeholder="sCol9"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="memberGameInfoStage[sCol10]" placeholder="sCol10"></textarea>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Access</h3>
                </div>
                <div class="box-body">
                    <div class="form-group">
                        <label>
                            <input type="radio" name="memberGameInfoStage[HideYN]" value="Y" class="minimal" checked> Hide Yes
                        </label>
                        &nbsp;
                        <label>
                             <input type="radio" name="memberGameInfoStage[HideYN]" value="N" class="minimal"> Hide No
                        </label>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="memberGameInfoStage[DeleteYN]" value="Y" class="minimal" checked> Delete  Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="memberGameInfoStage[DeleteYN]" value="N" class="minimal"> Delete No
                        </label>
                    </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "memberGameInfoStage"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
