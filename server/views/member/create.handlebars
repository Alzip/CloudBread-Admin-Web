<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New Member
        <small>Create a new Member</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'user.list'}}}">List</a></li>
        <li class="active">New Member</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'member.store'}}}" method="post">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Credentials</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[MemberID]" placeholder="Member ID"
                               required/>
                        <span class="glyphicon glyphicon-user form-control-feedback"></span>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="password" class="form-control" name="member[MemberPWD]" placeholder="Password"
                               required/>
                        <input type="password" class="form-control" name="member[MemberPWD2]"
                               placeholder="Retype password" required/>
                        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Email</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="email" class="form-control" name="member[EmailAddress]" placeholder="Email" required/>
                        <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Privates</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="tel" class="form-control" name="member[PhomeNumber1]" placeholder="PhoneNumber1" required/>
                        <input type="tel" class="form-control" name="member[PhomeNumber2]" placeholder="PhoneNumber2" required/>
                        <span class="glyphicon glyphicon-phone form-control-feedback"></span>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[Name1]" placeholder="Name1" />
                        <input type="text" class="form-control" name="member[Name2]" placeholder="Name2" />
                        <input type="text" class="form-control" name="member[Name3]" placeholder="Name3" />
                        <span class="glyphicon glyphicon-log-in form-control-feedback"></span>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="tel" class="form-control" name="member[DOB]" placeholder="Date of Birth"/>
                        <span class="glyphicon glyphicon-calendar form-control-feedback"></span>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Access</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[LastDeviceID]" placeholder="LastDeviceID" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[LastIPaddress]" placeholder="LastIPaddress" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[LastLoginDT]" placeholder="LastLoginDT" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[LastLogoutDT]" placeholder="LastLogoutDT" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[LastMACAddress]" placeholder="LastMACAddress" />
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Block</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[AccountBlockEndDT]" placeholder="AccountBlockEndDT" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[AccountBlockYN]" placeholder="AccountBlockYN" />
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">3rd Party Authentication</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[3rdAuthProvider]" placeholder="3rdAuthProvider" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[3rdAuthID]" placeholder="3rdAuthID" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[3rdAuthParam]" placeholder="3rdAuthParam" />
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Push Notification </h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[PushNotificationID]" placeholder="PushNotificationID" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[PushNotificationProvider]" placeholder="PushNotificationProvider" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="member[PushNotificationGroup]" placeholder="PushNotificationGroup" />
                    </div>
                </div>
            </div>
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Extra Data</h3>
                    </div>
                    <div class="box-body">
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol1]" placeholder="sCol1" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol2]" placeholder="sCol2" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol3]" placeholder="sCol3" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol4]" placeholder="sCol4" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol5]" placeholder="sCol5" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol6]" placeholder="sCol6" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol7]" placeholder="sCol7" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol8]" placeholder="sCol8" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol9]" placeholder="sCol9" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="member[sCol10]" placeholder="sCol10" />
                        </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "member"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
