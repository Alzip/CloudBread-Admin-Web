<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New App System Log
        <small>Create a App System Log</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'appSystemLog'}}}">List</a></li>
        <li class="active">New App System Log</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'appSystemLog.store'}}}" method="post">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Basic Info</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[PartitionKey]" placeholder="PartitionKey" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[RowKey]" placeholder="RowKey" required/>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Category Info</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[Category1]" placeholder="Category1" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[Category2]" placeholder="Category2"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[Category3]" placeholder="Category3"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[Category4]" placeholder="Category4"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[Category5]" placeholder="Category5"/>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Log</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="appSystemLog[LogDescription]" placeholder="LogDescription"></textarea>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Extra Data</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[sCol1]" placeholder="sCol1" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[sCol2]" placeholder="sCol2" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[sCol3]" placeholder="sCol3" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[sCol4]" placeholder="sCol4" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[sCol5]" placeholder="sCol5" />
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Date Time</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="appSystemLog[TimeStamp]" placeholder="TimeStamp" required/>
                    </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "appSystemLog"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
