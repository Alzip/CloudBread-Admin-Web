<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New Member Game Info
        <small>Create a Member Item Purchase</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'memberItemPurchase'}}}">List</a></li>
        <li class="active">New Member Item Purchase</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'memberItemPurchase.store'}}}" method="post">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">ID Info</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[MemberItemPurchaseID]" placeholder="MemberItemPurchaseID" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[MemberID]" placeholder="MemberID" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[ItemListID]" placeholder="ItemListID" required/>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Basic Info</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[PurchasePrice]" placeholder="PurchasePrice" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[PurchaseQuantity]" placeholder="PurchaseQuantity" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[PGinfo1]" placeholder="PGinfo1"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[PGinfo2]" placeholder="PGinfo2"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[PGinfo3]" placeholder="PGinfo3"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[PGinfo4]" placeholder="PGinfo4"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="purchase[PGinfo5]" placeholder="PGinfo5"/>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Extra Data</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol1]" placeholder="sCol1"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol2]" placeholder="sCol2"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol3]" placeholder="sCol3"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol4]" placeholder="sCol4"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol5]" placeholder="sCol5"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol6]" placeholder="sCol6"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol7]" placeholder="sCol7"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol8]" placeholder="sCol8"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol9]" placeholder="sCol9"></textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="purchase[sCol10]" placeholder="sCol10"></textarea>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Access</h3>
                </div>
                <div class="box-body">
                    <div class="form-group">
                        <label>
                            <input type="radio" name="purchase[HideYN]" value="Y" class="minimal" checked> Hide Yes
                        </label>
                        &nbsp;
                        <label>
                             <input type="radio" name="purchase[HideYN]" value="N" class="minimal"> Hide No
                        </label>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="purchase[DeleteYN]" value="Y" class="minimal" checked> Delete  Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="purchase[DeleteYN]" value="N" class="minimal"> Delete No
                        </label>
                    </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "memberItemPurchase"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
