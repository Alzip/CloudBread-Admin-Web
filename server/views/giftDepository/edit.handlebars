<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New Gift Depository
        <small>Create a new game event member</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'giftDepository'}}}">List</a></li>
        <li class="active">New Gift Depository</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'giftDepository.update'}}}" method="post">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Gift Depository Info</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[giftDepositoryID]" value="{{ obj.giftDepositoryID}}" placeholder="giftDepositoryID"
                               readonly/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[ItemListID]" value="{{ obj.ItemListID}}" placeholder="ItemListID" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[ItemCount]" value="{{ obj.ItemCount}}" placeholder="ItemCount" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[FromMemberID]" value="{{ obj.FromMemberID}}" placeholder="FromMemberID" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[ToMemberID]" value="{{ obj.ToMemberID}}" placeholder="ToMemberID" required/>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Extra Data</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol1]" placeholder="sCol1">{{ obj.sCol1}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol2]" placeholder="sCol2">{{ obj.sCol2}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol3]" placeholder="sCol3">{{ obj.sCol3}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol4]" placeholder="sCol4">{{ obj.sCol4}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol5]" placeholder="sCol5">{{ obj.sCol5}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol6]" placeholder="sCol6">{{ obj.sCol6}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol7]" placeholder="sCol7">{{ obj.sCol7}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol8]" placeholder="sCol8">{{ obj.sCol8}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol9]" placeholder="sCol9">{{ obj.sCol9}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="giftDepository[sCol10]" placeholder="sCol10">{{ obj.sCol10}}</textarea>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Access</h3>
                </div>
                <div class="box-body">
                    <div class="form-group">
                        <label>
                            <input type="radio" name="giftDepository[SentMemberYN]" value="{{ obj.SentMemberYN}}" class="minimal" checked> Sent Member Yes
                        </label>
                        &nbsp;
                        <label>
                             <input type="radio" name="giftDepository[SentMemberYN]" value="{{ obj.SentMemberYN}}" class="minimal"> Sent Member No
                        </label>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="giftDepository[HideYN]" value="{{ obj.HideYN}}" class="minimal" checked> Hide Yes
                        </label>
                        &nbsp;
                        <label>
                             <input type="radio" name="giftDepository[HideYN]" value="{{ obj.HideYN}}" class="minimal"> Hide No
                        </label>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="giftDepository[DeleteYN]" value="{{ obj.DeleteYN}}" class="minimal" checked> Delete  Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="giftDepository[DeleteYN]" value="{{ obj.DeleteYN}}" class="minimal"> Delete No
                        </label>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[CreatedAt]" value="{{ obj.CreatedAt}}" placeholder="CreatedAt" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[UpdatedAt]" value="{{ obj.UpdatedAt}}" placeholder="UpdatedAt" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[DataFromRegion]" value="{{ obj.DataFromRegion}}" placeholder="DataFromRegion"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="giftDepository[DataFromRegionDT]" value="{{ obj.DataFromRegionDT}}" placeholder="DataFromRegionDT"/>
                    </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "giftDepository"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
