<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New Game Event 
        <small>Create a new game event </small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'gameEvent'}}}">List</a></li>
        <li class="active">New Game Event </li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'coupon.store'}}}" method="post">
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Game Event Basic Info</h3>
                    </div>
                    <div class="box-body">
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[GameEventID]" placeholder="GameEventID"
                                   required/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[eventCategory1]" placeholder="eventCategory1" required/>
                            <input type="text" class="form-control" name="gameEvent[eventCategory2]" placeholder="eventCategory2"/>
                            <input type="text" class="form-control" name="gameEvent[eventCategory3]" placeholder="eventCategory3"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[ItemListID]" placeholder="ItemListID" required/>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Item Info</h3>
                    </div>
                    <div class="box-body">
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[ItemCount]" placeholder="ItemCount" required/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[ItemStatus]" placeholder="ItemStatus"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[TargetGroup]" placeholder="TargetGroup"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[TargetOS]" placeholder="TargetOS"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[TargetDevice]" placeholder="TargetDevice"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[EventImageLink]" placeholder="EventImageLink"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[Title]" placeholder="Title" required/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[Content]" placeholder="Content" required/>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Extra Data</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol1]" placeholder="sCol1" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol2]" placeholder="sCol2" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol3]" placeholder="sCol3" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol4]" placeholder="sCol4" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol5]" placeholder="sCol5" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol6]" placeholder="sCol6" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol7]" placeholder="sCol7" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol8]" placeholder="sCol8" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol9]" placeholder="sCol9" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol10]" placeholder="sCol10" />
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Access</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[CreateAdminID]" placeholder="CreateAdminID" required/>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="gameEvent[HideYN]" class="minimal" checked> Hide Yes
                        </label>
                        &nbsp;
                        <label>
                             <input type="radio" name="gameEvent[HideYN]" class="minimal"> Hide No
                        </label>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="gameEvent[DeleteYN]" class="minimal" checked> Delete  Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="gameEvent[DeleteYN]" class="minimal"> Delete No
                        </label>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[OrderNumber]" placeholder="OrderNumber" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[EventDurationFrom]" placeholder="EventDurationFrom" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[EventDurationTo]" placeholder="EventDurationTo" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[CreatedAt]" placeholder="CreatedAt" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[UpdatedAt]" placeholder="UpdatedAt" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[DataFromRegion]" placeholder="DataFromRegion"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[DataFromRegionDT]" placeholder="DataFromRegionDT"/>
                    </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "gameEvent"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
