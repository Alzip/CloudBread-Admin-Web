<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New Game Event 
        <small>Create a new game event </small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'gameEvent'}}}">List</a></li>
        <li class="active">New Game Event </li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'gameEvent.update'}}}" method="post">
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Game Event Basic Info</h3>
                    </div>
                    <div class="box-body">
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[GameEventID]" value="{{ obj.GameEventID}}" placeholder="GameEventID"
                                   readonly/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[EventCategory1]" value="{{ obj.EventCategory1}}" placeholder="eventCategory1" required/>
                            <input type="text" class="form-control" name="gameEvent[EventCategory2]" value="{{ obj.EventCategory2}}" placeholder="eventCategory2"/>
                            <input type="text" class="form-control" name="gameEvent[EventCategory3]" value="{{ obj.EventCategory3}}" placeholder="eventCategory3"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[ItemListID]" value="{{ obj.ItemListID}}" placeholder="ItemListID" required/>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Item Info</h3>
                    </div>
                    <div class="box-body">
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[ItemCount]" value="{{ obj.ItemCount}}" placeholder="ItemCount" required/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[ItemStatus]" value="{{ obj.ItemStatus}}" placeholder="ItemStatus"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[TargetGroup]" value="{{ obj.TargetGroup}}" placeholder="TargetGroup"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[TargetOS]" value="{{ obj.TargetOS}}" placeholder="TargetOS"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[TargetDevice]" value="{{ obj.TargetDevice}}" placeholder="TargetDevice"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[EventImageLink]" value="{{ obj.EventImageLink}}" placeholder="EventImageLink"/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[Title]" value="{{ obj.Title}}" placeholder="Title" required/>
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="gameEvent[Content]" value="{{ obj.Content}}" placeholder="Content" required/>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Extra Data</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol1]" value="{{ obj.sCol1}}" placeholder="sCol1" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol2]" value="{{ obj.sCol2}}" placeholder="sCol2" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol3]" value="{{ obj.sCol3}}" placeholder="sCol3" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol4]" value="{{ obj.sCol4}}" placeholder="sCol4" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol5]" value="{{ obj.sCol5}}" placeholder="sCol5" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol6]" value="{{ obj.sCol6}}" placeholder="sCol6" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol7]" value="{{ obj.sCol7}}" placeholder="sCol7" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol8]" value="{{ obj.sCol8}}" placeholder="sCol8" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol9]" value="{{ obj.sCol9}}" placeholder="sCol9" />
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[sCol10]" value="{{ obj.sCol10}}" placeholder="sCol10" />
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Access</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[CreateAdminID]" value="{{ obj.CreateAdminID}}" placeholder="CreateAdminID" readonly/>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="gameEvent[HideYN]" class="minimal" value="Y"
                                {{#isYes obj.HideYN}} {{/isYes}}
                            > Hide Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="gameEvent[HideYN]" class="minimal" value="N"
                                {{#isNo obj.HideYN}} {{/isNo}}
                            > Hide No
                        </label>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="gameEvent[DeleteYN]" class="minimal" value="Y"
                                {{#isYes obj.DeleteYN}} {{/isYes}}
                            > Delete  Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="gameEvent[DeleteYN]" class="minimal" value="N"
                                {{#isNo obj.DeleteYN}} {{/isNo}}
                            > Delete No
                        </label>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="gameEvent[OrderNumber]" value="{{ obj.OrderNumber}}" placeholder="OrderNumber" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control datetimepicker" name="gameEvent[EventDurationFrom]" value="{{ formatTimeDate obj.EventDurationFrom}}" placeholder="EventDurationFrom" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control datetimepicker" name="gameEvent[EventDurationTo]" value="{{ formatTimeDate obj.EventDurationTo}}" placeholder="EventDurationTo" required/>
                    </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "gameEvent"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
