<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New CouponMember
        <small>Create a new CouponMember</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'couponMember'}}}">List</a></li>
        <li class="active">New Coupon Member</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'couponMember.update'}}}" method="post">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">CouponMember Basic Info</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="CouponMember[CouponMemberID]" value="{{ obj.CouponMemberID}}" placeholder="CouponMember ID"
                               readonly/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="CouponMember[CouponID]" value="{{ obj.CouponID}}" placeholder="Coupon ID" required/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="CouponMember[MemberID]" value="{{ obj.MemberID}}" placeholder="MemberID"/>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Extra Data</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol1]"  placeholder="sCol1">{{ obj.sCol1}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol2]"  placeholder="sCol2">{{ obj.sCol2}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol3]"  placeholder="sCol3">{{ obj.sCol3}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol4]"  placeholder="sCol4">{{ obj.sCol4}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol5]"  placeholder="sCol5">{{ obj.sCol5}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol6]"  placeholder="sCol6">{{ obj.sCol6}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol7]"  placeholder="sCol7">{{ obj.sCol7}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol8]"  placeholder="sCol8">{{ obj.sCol8}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol9]"  placeholder="sCol9">{{ obj.sCol9}}</textarea>
                    </div>
                    <div class="form-group has-feedback">
                        <textarea class="form-control" name="CouponMember[sCol10]"  placeholder="sCol10">{{ obj.sCol10}}</textarea>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Access</h3>
                </div>
                <div class="box-body">
                    <div class="form-group">
                        <label>
                            <input type="radio" name="CouponMember[HideYN]" class="minimal" value="Y"
                                {{#isYes obj.HideYN}} {{/isYes}}
                            > Hide Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="CouponMember[HideYN]" class="minimal" value="N"
                                {{#isNo obj.HideYN}} {{/isNo}}
                            > Hide No
                        </label>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="CouponMember[DeleteYN]" class="minimal" value="Y"
                                {{#isYes obj.DeleteYN}} {{/isYes}}
                            > Delete  Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="CouponMember[DeleteYN]" class="minimal" value="N"
                                {{#isNo obj.DeleteYN}} {{/isNo}}
                            > Delete No
                        </label>
                    </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "couponMember"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
