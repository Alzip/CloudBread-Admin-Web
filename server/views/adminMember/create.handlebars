<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        New AdminMember
        <small>Create a new AdminMember</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{{url 'home'}}}"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="{{{url 'adminMember'}}}">List</a></li>
        <li class="active">New AdminMember</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- Input addon -->
            <!-- form -->
            <form action="{{{url 'adminMember.store'}}}" method="post">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Credentials</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="adminMember[AdminMemberID]" placeholder="AdminMember ID"
                               required/>
                        <span class="glyphicon glyphicon-user form-control-feedback"></span>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="password" class="form-control" name="adminMember[AdminMemberPWD]" placeholder="Password"
                               required/>
                        <input type="password" class="form-control" name="adminMember[AdminMemberPWD2]"
                               placeholder="Retype password" required/>
                        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Email</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="email" class="form-control" name="adminMember[AdminMemberEmail]" placeholder="Email" required/>
                        <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Privates</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="tel" class="form-control" name="adminMember[IDCreateAdminMember]" placeholder="IDCreateAdminMember"/>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="adminMember[Name1]" placeholder="Name1" />
                        <input type="text" class="form-control" name="adminMember[Name2]" placeholder="Name2" />
                        <input type="text" class="form-control" name="adminMember[Name3]" placeholder="Name3" />
                        <span class="glyphicon glyphicon-log-in form-control-feedback"></span>
                    </div>
                    <div class="form-group has-feedback">
                        <input type="tel" class="form-control" name="adminMember[DOB]" placeholder="Date of Birth"/>
                        <span class="glyphicon glyphicon-calendar form-control-feedback"></span>
                    </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Access</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="adminMember[PINumber]" placeholder="PINumber" />
                    </div>
                </div>
            </div>
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Extra Data</h3>
                    </div>
                    <div class="box-body">
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol1]" placeholder="sCol1" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol2]" placeholder="sCol2" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol3]" placeholder="sCol3" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol4]" placeholder="sCol4" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol5]" placeholder="sCol5" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol6]" placeholder="sCol6" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol7]" placeholder="sCol7" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol8]" placeholder="sCol8" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol9]" placeholder="sCol9" />
                        </div>
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" name="adminMember[sCol10]" placeholder="sCol10" />
                        </div>
                </div>
            </div>
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">DateTime</h3>
                </div>
                <div class="box-body">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" name="adminMember[TimeZoneID]" placeholder="TimeZoneID"  value="Korea Standard Time" required/>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="adminMember[HideYN]" class="minimal" value="Y" checked> Hide Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="adminMember[HideYN]" class="minimal" value="N" > Hide No
                        </label>
                    </div>
                    <div class="form-group">
                        <label>
                            <input type="radio" name="adminMember[DeleteYN]" class="minimal" value="Y" checked> Delete  Yes
                        </label>
                        &nbsp;
                        <label>
                            <input type="radio" name="adminMember[DeleteYN]" class="minimal" value="N" > Delete No
                        </label>
                    </div>
                </div>
            </div>
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-default" onclick="location.href = '{{{url "adminMember"}}}'">
                        Cancel
                    </button>
                </div>
                <!-- /.box-footer -->
            </form>
            <!-- /form -->
            <!-- /.box -->
        </div>
        <!-- /.col-md-6 -->
    </div>
    <!-- /.row -->
</section>
<!-- /.section -->
