    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "gaze": "^0.5.2",
    "git-repository": "^0.1.1",
    "jest-cli": "^0.8.2",
    "jest-webpack-alias": "^2.0.0",
    "json-loader": "^0.5.4",
