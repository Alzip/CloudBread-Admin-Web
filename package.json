{
  "private": true,
  "scripts": {
    "test": "npm run lint && $(npm bin)/jest",
    "start": "$(npm bin)/babel-node tools/run dev",
    "build": "npm run test && $(npm bin)/babel-node tools/run build -- --release",
    "lint": "$(npm bin)/eslint ./src ./tools",
    "clean": "$(npm bin)/babel-node tools/run clean",
    "deploy": "npm run test && $(npm bin)/babel-node tools/run deploy -- --release"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "stage-2",
      "stage-3",
      "react"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "sinon"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "dependencies": {
    "autoprefixer": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "bluebird": "^3.1.1",
    "classnames": "^2.2.1",
    "express": "^4.13.3",
    "front-matter": "^2.0.1",
    "history": "^1.17.0",
    "jade": "^1.11.0",
    "material-ui": "^0.14.0",
    "normalize.css": "^3.0.3",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-router": "^2.0.0-rc4",
    "react-tap-event-plugin": "^0.2.1",
    "redux-simple-router": "^2.0.1",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.2.0",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
