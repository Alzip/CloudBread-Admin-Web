{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "quotes": [2, "single"],
    "curly": 2,
    "semi": [2, "always"],
    "new-cap": [
      2, {
        "capIsNewExceptions": ["Router"]
      }
    ]
  }
}
